<%= form_with(model: @feed, local: true) do |form| %>
  <div class="filed">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <div class="filed">
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>
  <%= form.submit %>
<% end %>
<%= link_to 'みんなの投稿をみよう！', feeds_path %>
